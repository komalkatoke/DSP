use mixed_batch

select * from employees

create table students
(studId int primary key, SNAme char(10), DOB date)

insert into students
values(1,'Harry','2000-01-01'),
	(2,'Tom','2000-02-02'),
    (3,'Harry','2000-03-03')


create table subjects
(subjectID int primary key, subjectName char(10))

insert into subjects
values(1,'Python'),(2,'R'),(3,'Tableau')
